  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "clean": "gulp clean:bundle",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint '{lib,commands,actions,configs,scripts}/**/*.ts' --fix",
    "dev": "ts-node bin/kunlun.ts",
    "predev:node": "npm run build",
    "dev:node": "node bin/kunlun.js",
    "start": "npm run build && node bin/kunlun.js",
    "prepack": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "jest --config test/jest-config.json",
    "test:dev": "npm run clean && jest --config test/jest-config.json --watchAll",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
